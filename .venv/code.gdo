zap fib(n) {
    if (n == 0) { return 0; }
    if (n == 1) { return 1; }
    return addNums(fib(n - 1), fib(n - 2));
}

zap factorial(n){
    if(n==0) { return 1; }
    return n * factorial(n-1);
}

x = fib(8);
print(x);
print(factorial(5));

